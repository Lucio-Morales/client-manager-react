{
  "compilerOptions": {
    "jsx": "react-jsx",
    "module": "ESNext",
    "lib": ["DOM", "es2020"],
    "target": "es2020",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  },
 "include": ["src"]
}
